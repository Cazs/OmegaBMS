<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fadulousbms.controllers.SuppliersController">
   <children>
      <HBox alignment="CENTER_LEFT" VBox.vgrow="NEVER">
         <children>
            <MenuBar HBox.hgrow="ALWAYS">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#createSupplierClick" text="New Supplier" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Selected Supplier">
                  <items>
                    <MenuItem mnemonicParsing="false" text="View" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </HBox>
      <BorderPane styleClass="sys-nav" stylesheets="@../styles/home.css">
         <bottom>
            <BorderPane style="-fx-border-insets: -50 -10 0 -10;" BorderPane.alignment="CENTER">
               <center>
                  <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Suppliers">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </center>
               <left>
                  <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
                     <children>
                        <ImageView fx:id="img_profile" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../images/profile.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="user_name" text="not signed in" HBox.hgrow="ALWAYS">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </left>
               <right>
                  <HBox alignment="CENTER_RIGHT" BorderPane.alignment="CENTER">
                     <children>
                        <Button alignment="CENTER" contentDisplay="CENTER" minHeight="35.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#showMain" styleClass="btnDefault" stylesheets="@../styles/home.css" text="home" />
                        <Button minHeight="35.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#showLogin" styleClass="btnAdd" stylesheets="@../styles/home.css" text="login">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button alignment="CENTER" contentDisplay="CENTER" minHeight="35.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#previousScreen" styleClass="btnBack" stylesheets="@../styles/home.css" text="Back" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
               </right>
            </BorderPane>
         </bottom>
         <top>
            <Label text="Airotek BMS" BorderPane.alignment="CENTER">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
         </top>
      </BorderPane>
      <TableView fx:id="tblSuppliers" editable="true" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="colSupplierId" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="-1.0" text="Client ID" visible="false">
              <cellValueFactory>
                  <PropertyValueFactory property="_id" />
              </cellValueFactory>
          </TableColumn>
          <TableColumn fx:id="colSupplierName" maxWidth="1.7976931348623157E308" minWidth="180.0" prefWidth="-1.0" text="Supplier Name">
              <cellValueFactory>
                  <PropertyValueFactory property="supplier_name" />
              </cellValueFactory>
          </TableColumn>
            <TableColumn fx:id="colSupplierPhysicalAddress" maxWidth="1.7976931348623157E308" minWidth="250.0" prefWidth="-1.0" text="Physical Address">
                <cellValueFactory>
                    <PropertyValueFactory property="physical_address" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierPostalAddress" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Postal Address" visible="true">
                <cellValueFactory>
                    <PropertyValueFactory property="postal_address" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierTel" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Tel">
                <cellValueFactory>
                    <PropertyValueFactory property="tel" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierFax" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Fax">
                <cellValueFactory>
                    <PropertyValueFactory property="fax" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierSpeciality" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Speciality">
                <cellValueFactory>
                    <PropertyValueFactory property="speciality" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierActive" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Active">
                <cellValueFactory>
                    <PropertyValueFactory property="active" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierDatePartnered" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Date Partnered">
                <cellValueFactory>
                    <PropertyValueFactory property="date_partnered" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierWebsite" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Website">
                <cellValueFactory>
                    <PropertyValueFactory property="website" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colSupplierOther" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Other">
                <cellValueFactory>
                    <PropertyValueFactory property="other" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="colAction" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Action">
            </TableColumn>
        </columns>
      </TableView>
      <HBox>
         <children>
            <Label text="Server Status:" />
            <Circle fx:id="shpServerStatus" fill="DODGERBLUE" radius="7.0" stroke="BLACK" strokeType="INSIDE" />
            <Label fx:id="lblOutput" text="console output">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</VBox>
