<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fadulousbms.controllers.RejectedQuotesController">
   <children>
      <HBox alignment="CENTER_LEFT" VBox.vgrow="NEVER">
         <children>
            <MenuBar HBox.hgrow="ALWAYS">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#newQuote" text="New Quote" />
                        <MenuItem mnemonicParsing="false" onAction="#newGenericQuote" text="New Generic Quote" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </HBox>
      <BorderPane styleClass="sys-nav" stylesheets="@../styles/home.css">
         <bottom>
            <BorderPane style="-fx-border-insets: -50 -10 0 -10;" BorderPane.alignment="CENTER">
               <center>
                  <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Rejected Quotes">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </center>
               <left>
                  <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
                     <children>
                        <ImageView fx:id="img_profile" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../images/profile.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="user_name" text="not signed in" HBox.hgrow="ALWAYS">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </left>
               <right>
                  <HBox alignment="CENTER_RIGHT" BorderPane.alignment="CENTER">
                     <children>
                        <Button alignment="CENTER" contentDisplay="CENTER" minHeight="35.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#showMain" styleClass="btnDefault" stylesheets="@../styles/home.css" text="home" />
                        <Button minHeight="35.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#showLogin" styleClass="btnAdd" stylesheets="@../styles/home.css" text="login">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button alignment="CENTER" contentDisplay="CENTER" minHeight="35.0" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#previousScreen" styleClass="btnBack" stylesheets="@../styles/home.css" text="Back" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
               </right>
            </BorderPane>
         </bottom>
         <top>
            <Label text="AIROTEK BMS" BorderPane.alignment="CENTER">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
         </top>
      </BorderPane>
      <VBox style="-fx-background-color: #7A7A7A;" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="tblRejectedQuotes" editable="true" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="colQuoteNumber" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="-1.0" text="Quote Number" visible="false">
                     <cellValueFactory>
                        <PropertyValueFactory property="_id" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colQuoteClient" maxWidth="1.7976931348623157E308" minWidth="180.0" prefWidth="-1.0" text="Client">
                     <cellValueFactory>
                        <PropertyValueFactory property="client" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colQuoteRequest" maxWidth="1.7976931348623157E308" minWidth="250.0" prefWidth="-1.0" text="Request">
                     <cellValueFactory>
                        <PropertyValueFactory property="request" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colGenericQuoteSitename" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Sitename">
                     <cellValueFactory>
                        <PropertyValueFactory property="sitename" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colQuoteContact" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Contact Person">
                     <cellValueFactory>
                        <PropertyValueFactory property="contact_person" />
                     </cellValueFactory>
                  </TableColumn>
                   <TableColumn fx:id="colCell" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Cell" visible="false">
                       <cellValueFactory>
                           <PropertyValueFactory property="cell" />
                       </cellValueFactory>
                   </TableColumn>
                   <TableColumn fx:id="colTel" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Tel" visible="false">
                       <cellValueFactory>
                           <PropertyValueFactory property="tel" />
                       </cellValueFactory>
                   </TableColumn>
                   <TableColumn fx:id="colEmail" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="eMail" visible="false">
                       <cellValueFactory>
                           <PropertyValueFactory property="email" />
                       </cellValueFactory>
                   </TableColumn>
                  <TableColumn fx:id="colQuoteDate" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Date Logged">
                     <cellValueFactory>
                        <PropertyValueFactory property="date_generated" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colQuoteStatus" prefWidth="75.0" text="Status" visible="false">
                     <cellValueFactory>
                        <PropertyValueFactory property="status" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colQuoteCreator" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Creator" />
                  <TableColumn fx:id="colQuoteExtra" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="-1.0" text="Extra" visible="false">
                     <cellValueFactory>
                        <PropertyValueFactory property="extra" />
                     </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colQuoteAction" maxWidth="1.7976931348623157E308" minWidth="220.0" prefWidth="-1.0" text="Action">
                     <cellValueFactory>
                        <PropertyValueFactory property="extra" />
                     </cellValueFactory>
                  </TableColumn>
               </columns>
            </TableView>
         </children>
      </VBox>
      <HBox>
         <children>
            <Label text="Server Status:" />
            <Circle fx:id="shpServerStatus" fill="DODGERBLUE" radius="7.0" stroke="BLACK" strokeType="INSIDE" />
            <Label fx:id="lblOutput" text="console output">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</VBox>
